# Server
ENV=dev
LOG_LEVEL=INFO

# CORS (comma-separated)
ALLOWED_ORIGINS=http://localhost:5173

# Simulation limits (security + stability)
MAX_TURN_LIMIT=50
MAX_AGENTS=10
MAX_TOPIC_CHARS=2000
MAX_PROMPT_CHARS=12000
MAX_STAGE_CHARS=2000
ORPHAN_GRACE_SECONDS=30

# Model catalog (optional override)
# Default is backend/model_catalog.json
MODEL_CATALOG_PATH=

# Env loading behavior (python-dotenv)
# - If true, values in backend/.env override already-set OS environment variables.
# - Recommended: leave empty/false for production safety.
DOTENV_OVERRIDE=false

# LLM Provider Keys (set the ones you need)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=


